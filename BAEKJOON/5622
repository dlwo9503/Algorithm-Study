#include <iostream>
#include <string>

using namespace std;

int main() {
	string s;
	int time = 0;
	cin >> s;
	for (int i = 0; i < s.length(); i++)
	{
		int a = s[i];
		if (a <= 67) time += 3;
		else if (a <= 70) time += 4;
		else if (a <= 73) time += 5;
		else if (a <= 76) time += 6;
		else if (a <= 79) time += 7;
		else if (a <= 83) time += 8;
		else if (a <= 86) time += 9;
		else if (a <= 90) time += 10;
		else time += 11;
	}
	cout << time;
}
